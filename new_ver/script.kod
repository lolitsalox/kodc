fib(n) {
	if n < 2 { return n }
	return fib(n - 1) + fib(n - 2)
}

fac(n) {
	if n < 2 { return 1 }
	return n * fac(n - 1)
}

i = 0
while i < 10 {
	print(fac(i))
	i = i + 1
}